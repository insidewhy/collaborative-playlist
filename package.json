{
  "name": "share-deezer",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "concurrently 'npm run client:run' 'npm run server:watch' 'npm run server:run'",
    "server:watch": "tsc -w -p server",
    "server:build": "tsc -p server",
    "server:run": "supervisor -t -n exit -w dist/server dist/server/app.js",
    "client:run": "ng serve --no-progress --host 0.0.0.0 --port 9000",
    "client:build": "ng build -prod",
    "build": "npm run client:build && npm run server:build",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.1.1",
    "@angular/common": "^4.1.1",
    "@angular/compiler": "^4.1.1",
    "@angular/core": "^4.1.1",
    "@angular/forms": "^4.1.1",
    "@angular/http": "^4.1.1",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "^4.1.1",
    "@angular/platform-browser-dynamic": "^4.1.1",
    "@angular/router": "^4.1.1",
    "core-js": "^2.4.1",
    "es6-promisify": "^5.0.0",
    "koa": "^2.0.0-alpha.8",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-websocket": "^3.0.1",
    "lodash": "^4.17.4",
    "mkpath": "^1.0.0",
    "queueing-subject": "^0.1.1",
    "rxjs": "^5.0.1",
    "rxjs-websockets": "^0.8.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.8.10"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.2",
    "@angular/compiler-cli": "^4.1.1",
    "@types/jasmine": "^2.5.47",
    "@types/koa": "^2.0.37",
    "@types/koa-router": "^7.0.21",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.18",
    "codelyzer": "3.0.1",
    "concurrently": "^3.3.0",
    "jasmine-core": "^2.6.1",
    "jasmine-spec-reporter": "^4.1.0",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "^1.0.2",
    "protractor": "~5.1.1",
    "supervisor": "^0.12.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "typescript": "^2.3.1"
  }
