{
  "name": "share-deezer",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "concurrently 'npm run client:run' 'npm run server:watch' 'npm run server:run'",
    "server:watch": "tsc -w -p server",
    "server:build": "tsc -p server",
    "server:run": "supervisor -t -n exit -w dist/server dist/server/app.js",
    "client:run": "ng serve --no-progress --host 0.0.0.0 --port 9000",
    "client:build": "ng build -prod",
    "build": "npm run client:build && npm run server:build",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@angular2-material/core": "^2.0.0-alpha.8-2",
    "@angular2-material/icon": "^2.0.0-alpha.8-2",
    "core-js": "^2.4.1",
    "es6-promisify": "^5.0.0",
    "koa": "^2.0.0-alpha.8",
    "koa-router": "^7.0.1",
    "koa-static": "^3.0.0",
    "koa-websocket": "^3.0.1",
    "lodash": "^4.17.4",
    "mkpath": "^1.0.0",
    "queueing-subject": "^0.1.1",
    "rxjs": "^5.0.1",
    "rxjs-websockets": "^0.8.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/cli": "1.0.0-beta.30",
    "@angular/compiler-cli": "^2.4.8",
    "@types/jasmine": "2.5.38",
    "@types/koa": "^2.0.37",
    "@types/koa-router": "^7.0.21",
    "@types/lodash": "4.14.50",
    "@types/node": "^6.0.63",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.3.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "supervisor": "^0.12.0",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.2.0-rc1"
  }
